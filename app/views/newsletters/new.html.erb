<div class="left_column_fm">  <h4><%= @title %></h4>  <%= form_for :newsletter do |f| %>    <p>      <%= f.label :title %>: <%= f.text_field :title %>    </p>    <p>      <%= f.label :month %>: <%= f.text_field :month %>    </p>    <p>      <%= f.label :year %>: <%= f.text_field :year %>    </p>    <p>      <%= f.submit %>    </p>  <% end %><p>http://guides.rubyonrails.org/getting_started.html</p></div>