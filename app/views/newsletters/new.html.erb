<div class="left_column_fm">  <h4><%= @title %></h4>  <%= form_for :newsletter, url: newsletters_path do |f| %>    <p>      <%= f.label :title %>:<%= f.text_field :title, size: 50 %>    </p>    <p>      <%= f.label :month %>:<%= f.text_field :month, size: 50 %>    </p>    <p>      <%= f.label :year %>: <%= f.text_field :year, size: 50 %>    </p><br>    <p>      <%= f.submit "Add Newsletter" %>    </p>  <% end %></div>